<div class="col-md-12">

 <h2 class="text-center"> 我的订单 </h2>

  <table class="table">
    <thead>
      <tr>
        <th> # </th>
        <th> 订购人 </th>
        <th> 创建时间 </th>
        <th> 更新时间 </th>
        <th colspan="2"></th>
      </tr>
    </thead>

    <tbody>
      <% @orders.each do |order| %>
        <tr>
          <td> <%= order.id %> </td>
          <td> <%= order.billing_name %> </td>
          <td> <%= order.created_at %> </td>
          <td> <%= order.updated_at %> </td>
        </tr>
      <% end %>
     </tbody>
  </table>
</div>
